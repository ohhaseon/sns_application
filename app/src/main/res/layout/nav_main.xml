<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navRightView"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/txtNickname0"
        android:layout_width="200dp"
        android:layout_height="0dp"
        android:layout_marginTop="120dp"
        android:layout_marginBottom="102dp"
        android:gravity="center"
        android:text="내정보_닉네임"
        app:layout_constraintBottom_toTopOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.26" />


    <TextView
        android:id="@+id/txtEmail0"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginBottom="291dp"
        android:background="#0C000F90"
        android:gravity="center"
        android:text="내정보_이메일"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view" />

    <View
        android:id="@+id/view"
        android:layout_width="200dp"
        android:layout_height="0dp"
        android:layout_marginTop="162dp"
        android:layout_marginBottom="7dp"
        android:background="#90909090"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/txtEmail0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/EditTextNickname1"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginBottom="231dp"
        android:gravity="center"

        android:inputType="textPersonName"
        android:maxLength="45"
        android:textSize="15sp"
        app:layout_constraintTop_toBottomOf="@+id/txtEmail0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/txtNickname1"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginBottom="231dp"
        android:gravity="center"

        android:text="내정보_닉네임"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNickname0"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/view2"
        android:layout_width="200dp"
        android:layout_height="1dp"
        android:layout_marginBottom="238dp"
        android:background="#90909090"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNickname0" />

    <TextView
        android:id="@+id/txtEmail1"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:background="#0C000F90"
        android:gravity="center"
        android:text="내정보_이메일"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNickname1" />

</android.support.constraint.ConstraintLayout>